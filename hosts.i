pFileList dirfiles(char const *dir_path, char *passhosts[])
{
 DIR *dir;
 struct dirent *pinfo = ((void *)0);
 if((dir = opendir(dir_path)) == ((void *)0))
 {
  return ((void *)0);
 }
 pFileList pHead = (pFileList)malloc(sizeof(FileList));
 pHead->next = ((void *)0);
 pFileList pTail = pHead;
 while((pinfo = readdir(dir)) != ((void *)0))
 {

  if(pinfo->d_type != DT_REG)
   continue;
  if(passfile(pinfo->d_name, passhosts, 3) == 1)
   continue;
  pFileList pNew = (pFileList)malloc(sizeof(FileList));
  pNew->d_name = pinfo->d_name;
  pNew->next = ((void *)0);
  pTail->next = pNew;
  pTail = pTail->next;
  pinfo = ((void *)0);
 }
 return pHead;
}

static int copyfile(char const *src, char const *dest)
{
 FILE *srcfp,*destfp;
 if((srcfp = fopen(src, "r")) == ((void *)0))
  return 1;
 if((destfp = fopen(dest, "w")) == ((void *)0))
  return 1;
 char buf[1024];
 while(fgets(buf, 1024, srcfp) != ((void *)0))
 {
  fputs(buf, destfp);
 }
 fclose(srcfp);
 fclose(destfp);
 return 0;
}

