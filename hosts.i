void formatline(char *buf, char *buf_tmp)
{
 int i = 0, len;
 len = strlen(buf);
 while(i <= len)
 {
  if(buf[i] == '\n' || buf[i] == '\0')
  {
   i++;
   continue;
  }
  if(buf[i] == '\t')
  {
   buf_tmp[i] = ' ';
  }
  else
  {
   buf_tmp[i] = buf[i];
  }
  i++;
 }
 buf_tmp[i] = '\0';
}

