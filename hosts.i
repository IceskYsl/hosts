int delhosts(char const *ip, char const *web, char const *path)
{
 char path_tmp[256] = {0};
 sprintf(path_tmp, "%s%s", path, "_tmp");
 FILE *fp,*fp_tmp;
    if((fp_tmp = fopen(path_tmp, "w+")) == ((void *)0))
    {
        printf("打开临时hosts配置文件失败！\n");
        return 1;
    }
    if((fp = fopen(path, "r")) == ((void *)0))
    {
        printf("打开当前hosts配置文件失败！");
        return 1;
    }
 char buf[1024] = {0};
 int i,j,len,flag;
 char item[1024] = {0};
 char buf_tmp[1024] = {0};
 while(fgets(buf, 1024, fp) != ((void *)0))
 {
  i = 0;
  j = 0;
  flag = 0;
  len = strlen(buf);
  memset(item, 0, 1024);
  memset(buf_tmp, 0, 1024);
  while(i < len)
  {
            if(buf[i] != ' ' && buf[i] != '\t' && buf[i] != '\n')
            {
                item[j] = buf[i];
            }
   else
   {
    item[j] = '\0';
    if(item[0] != '\0')
    {
     if(!flag)
     {
      if(strcmp(item, ip))
      {
       strcpy(buf_tmp, buf);
       break;
      }
      else
      {
       if(web == ((void *)0))
       {
        buf_tmp[0] = '\0';
        break;
       }
       else
       {
        strcat(buf_tmp, item);
        strcat(buf_tmp, " ");
        flag = 1;
       }
      }
     }
     else
     {
      if(strcmp(web, item))
      {
       strcat(buf_tmp, item);
       strcat(buf_tmp, " ");
      }
     }
    }
    memset(item, 0 ,1024);
   }
   if(i == (len - 1))
    strcat(buf_tmp, "\n");
   i++;
   j++;
  }
  fputs(buf_tmp, fp_tmp);
 }
 fclose(fp);
 fclose(fp_tmp);
 copyfile(path_tmp, path);
 copyfile(path, "/etc/hosts");
 remove(path_tmp);
 return 0;
}

int parse_command(int argc, char **argv)
{
    if(argc == 1)
        return 1;
    char *command = argv[1];
    if(!strcmp(command, "all"))
        return 2;
    else if(!strcmp(command, "change"))
        return 3;
    else if(!strcmp(command, "add"))
        return 4;
    else if(!strcmp(command, "del"))
        return 5;
    else if(!strcmp(command, "make"))
        return 6;
    else if(!strcmp(command, "names"))
        return 7;
    else if(!strcmp(command, "remove"))
        return 8;
    return 0;
}

